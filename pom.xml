              <modelPackage>${project.groupId}.microservice.msclient.generated.delivery.io.v1.dto</modelPackage>
              <apiPackage>${project.groupId}.microservice.msclient.generated.delivery.io.v1.api</apiPackage>
              <modelNameSuffix />
            </configuration>
